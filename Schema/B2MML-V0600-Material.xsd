      <xsd:element name = "MaterialDefinitionID"        type = "MaterialDefinitionIDType"       
                                                        minOccurs = "0"/>
      <xsd:element name = "Status"                      type = "StatusType"
                                                        minOccurs = "0"/>
      <xsd:element name = "MaterialLotProperty"         type = "MaterialLotPropertyType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "MaterialSubLot"              type = "MaterialSubLotType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "StorageLocation"             type = "StorageHierarchyScopeType"    
                                                        minOccurs = "0"/>
      <xsd:element name = "Quantity"                    type = "QuantityValueType" 
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "MaterialTestSpecificationID" type = "MaterialTestSpecificationIDType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "AssemblyLotID"               type = "MaterialLotType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "AssemblySubLotID"            type = "MaterialSubLotType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "AssemblyType"                type = "AssemblyTypeType" 
	                                                    minOccurs = "0"/>
      <xsd:element name = "AssemblyRelationship"        type = "AssemblyRelationshipType" 
	                                                    minOccurs = "0"/>
      <xsd:group   ref  = "Extended:MaterialLot"        minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name = "MaterialLotPropertyType">
    <xsd:sequence>
      <xsd:element name = "ID"                          type = "IdentifierType"/>
      <xsd:element name = "Description"                 type = "DescriptionType"        
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "Value"                       type = "ValueType"      
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "MaterialLotProperty"         type = "MaterialLotPropertyType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "MaterialTestSpecificationID" type = "MaterialTestSpecificationIDType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "TestResult"                  type = "TestResultType" 
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:group   ref  = "Extended:MaterialLotProperty" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name = "MaterialSubLotType">
    <xsd:sequence>
      <xsd:element name = "ID"                          type = "IdentifierType"/>
      <xsd:element name = "Description"                 type = "DescriptionType" 
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
